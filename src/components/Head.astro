---
import '@fontsource/roboto/300.css';
import '@fontsource/roboto/400.css';
import '@fontsource/roboto/500.css';
import '@fontsource/roboto/700.css';

import Favicon from './Favicon.astro';
import Seo from './Seo.astro';
---

<!-- Global Metadata -->
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width" />

<Favicon />
<link rel="mask-icon" href="/favicon.ico" />
<link rel="sitemap" href="/sitemap-index.xml" />

<Seo />

<!-- Prefetch page -->
<link rel="prefetch" href="downloads" crossorigin="anonymous" />

<link
  rel="preconnect"
  href="https://cdnjs.cloudflare.com"
  crossorigin="anonymous"
/>
<link
  rel="dns-prefetch"
  href="https://cdnjs.cloudflare.com"
  crossorigin="anonymous"
/>

<!-- Fathom - beautiful, simple website analytics -->
<!-- <script
  src="https://certain-quality.astro.build/script.js"
  data-site="ABCDEFGH"
  defer
></script> -->

<!-- Font preloads -->

<!-- Images preloads -->
<link
  rel="preload"
  as="image"
  href="/assets/images/logo.png"
  crossorigin="anonymous"
/>

<!-- CSS preloads -->
<link
  rel="preload"
  as="style"
  href="https://cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.1.1/cookieconsent.min.css"
  crossorigin="anonymous"
/>

<!-- JS preloads -->
<link
  rel="preload"
  as="script"
  href="https://cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.1.1/cookieconsent.min.js"
  crossorigin="anonymous"
/><link rel="preload" as="script" href="/assets/js/custom.min.js" />
